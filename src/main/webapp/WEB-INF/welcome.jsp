<script type="text/javascript">
$(document).ready(function(){
	$("#queryButton").click(function(){
		if($("#server").val()==""||$("#role").val()==""){
			$("#message").html("请输入服务器和角色名");
			return;
		}
		$("#message").html("角色查找中···");
		$.get("/wow/query",{"server": $("#server").val(),"name": $("#role").val(),"random":Math.random()},function(json){
			var result = eval("(" +json + ")");
			if(json){
				$("#message").html("角色已找到,生成角色快照···");
				build();
			}else{
				$("#message").html("角色不存在：名字不对/等级不够/太久没登录");
			}
			
		 });
	});	
});
	
function build(){
	$.get("/wow/build",{"server": $("#server").val(),"name": $("#role").val(),"random":Math.random()},function(json){
		var result = eval("(" +json + ")");
		if(json){
			$("#message").html("角色快照完成,图片生成中···"+result.uuid);
			$("#uuid").val(result.uuid);
			show();
		}else{
			$("#message").html("角色快照生成失败，请重试");
		}
		
	 });
}
function show(){
	$.get("/wow/show",{"uuid": $("#uuid").val(),"name": $("#role").val(),"random":Math.random()},function(json){
		var result = eval("(" +json + ")");
		if(json){
			$("#message").html("<img src='"+result.url+"'/>");
		}else{
			$("#message").html("图片生成失败，请重试");
		}
		
	 });
}
</script>